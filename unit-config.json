{
	"listeners": {
		"*:80": {
			"pass": "routes"
		}
	},
	"routes": [
		{
			"match": {
				"uri": [
					"*.js"
				]
			},
			"action": {
				"share": [
					"/var/www/html/$uri"
				]
			}
		},
		{
			"match": {
				"uri": [
					"/"
				]
			},
			"action": {
				"pass": "applications/test/main"
			}
		},
		{
			"match": {
				"uri": [
					"/results/telemetry.php"
				]
			},
			"action": {
				"pass": "applications/test/telemetry"
			}
		}
	],


"applications": {
        "test": {
            "type": "php",
            "targets": {
                "telemetry": {
                    "root": "/var/www/html/"
                },

                "main": {
                    "root": "/var/www/html/",
                    "script": "index.php"
                }
            }
        }
    }






}
